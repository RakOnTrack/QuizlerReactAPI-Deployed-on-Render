{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run dev",
      "name": "npm dev",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
